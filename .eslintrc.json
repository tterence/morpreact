    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack.common.js"
            }
        }
    }
}